<div class="container">
    <div class="row">
        {{#each products}}
            <div class="col-md-4">
                <div class="card">
                    <img src={{this.thumbnails}} class="card-img-top" alt={{this.title}}>
                    <div class="card-body">
                        <h5 class="card-title">{{this.title}}</h5>
                        <p class="card-text">${{this.price}}</p>
                        <a href="#" class="btn btn-dark">Ver m√°s</a>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>